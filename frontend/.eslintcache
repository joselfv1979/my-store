   if (validateForm(user)) clearForm()\n    }\n\n    const clearForm = () => {\n\n        document.getElementById(\"username\").value = '';\n        document.getElementById(\"email\").value = '';\n        document.getElementById(\"password\").value = '';\n    }\n\n    return (\n        <form onSubmit={handleFormSubmit}>\n            <fieldset>\n                <label htmlFor=\"username\">Username:</label>\n                <input type=\"text\"\n                    name=\"username\"\n                    id=\"username\"\n                    required\n                    autoFocus\n                    onChange={event =>\n                        setUsername(event.target.value)}\n                />\n            </fieldset>\n            <fieldset>\n                <label htmlFor=\"email\">Email:</label>\n                <input type=\"text\"\n                    name=\"email\"\n                    id=\"email\"\n                    required\n                    onChange={event =>\n                        setEmail(event.target.value)}\n                />\n            </fieldset>\n            {showPasswordInput ? (\n                <fieldset>\n                    <label htmlFor=\"password\">Password:</label>\n                    <input type=\"password\"\n                        name=\"password\"\n                        id=\"password\"\n                        required\n                        onChange={event =>\n                            setPassword(event.target.value)}\n                    />\n                </fieldset>\n            ) : null}\n\n            <button>Submit</button>\n        </form>\n    )\n}\n\nexport default RegisterForm;","/home/jose/Documentos/projects/my-store/frontend/src/components/Menu.js",["94"],"import React, { useContext, useEffect } from 'react';\nimport { NavLink, useHistory } from 'react-router-dom';\nimport { UserContext } from '../context/UserContext';\n\nconst Menu = () => {\n\n    const { username, setUsername, role, setRole, setIsAdmin } 